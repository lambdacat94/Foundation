                            <TextBox x:Name="TbxYoungIntenMin" HorizontalAlignment="Left" Margin="342.588,50.091,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="105" RenderTransformOrigin="-0.343,2.616" Height="22"/>
