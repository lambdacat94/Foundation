                            <TextBox x:Name="TbxCulIntenMin" HorizontalAlignment="Left" Width="45" Margin="342.588,114.277,0,0" VerticalAlignment="Top" PreviewKeyDown="TbxCulIntenMin_PreviewKeyDown" Height="22" />
