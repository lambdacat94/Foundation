                    <Button x:Name="BtnInsertNames" Content="选择名称文件" HorizontalAlignment="Left" Margin="425.911,7.492,0,0" VerticalAlignment="Top" Width="87" Click="BtnInsertNames_Click"/>


                </Grid>
            </TabItem>
