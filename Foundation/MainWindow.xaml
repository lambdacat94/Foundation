                                <GridViewColumn Header="重点支持受理" Width="160" DisplayMemberBinding="{Binding KeyAcceptCount}"/>
                                <GridViewColumn Header="培育受理" Width="160" DisplayMemberBinding="{Binding CulAcceptCount}" />
